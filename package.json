{
  "name": "bitbucket",
  "version": "0.1.0",
  "description": "BitBucket API client for Browser and Node.js",
  "keywords": ["bitbucket", "api-client"],
  "homepage": "https://github.com/MunifTanjim/node-bitbucket#readme",
  "bugs": "https://github.com/MunifTanjim/node-bitbucket/issues",
  "license": "MIT",
  "author": "MunifTanjim (https://muniftanjim.com)",
  "files": ["index.js", "libs", "dist/*.js"],
  "main": "index.js",
  "repository": "github:MunifTanjim/node-bitbucket",
  "scripts": {
    "build": "npm-run-all build:*",
    "prebuild:browser": "mkdirp dist/",
    "build:browser": "npm-run-all build:browser:*",
    "build:browser:development":
      "webpack --config scripts/webpack.config.js --mode development",
    "build:browser:production":
      "webpack --config scripts/webpack.config.js --mode production --output-filename=bitbucket.min.js",
    "get-api-spec": "node scripts/get-api-spec.js",
    "generate-methods-list": "node scripts/generate-methods-list.js",
    "generate-routes": "node scripts/generate-routes.js",
    "publish": "npm run build && npm publish"
  },
  "dependencies": {
    "before-after-hook": "^1.1.0",
    "btoa-lite": "^1.0.0",
    "deepmerge": "^2.1.0",
    "node-fetch": "^2.1.2",
    "url-template": "^2.0.8"
  },
  "devDependencies": {
    "debug": "^3.1.0",
    "lodash": "^4.17.10",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.3",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3"
  }
}
